#: app/forms.py:45
msgid "Please, inform a valid email"
msgstr "Por favor, informe um email válido"

#: app/forms.py:48
msgid "Message"
msgstr "Mensagem"

#: app/forms.py:49
msgid "Please, write your message"
msgstr "Por favor, escreva sua mensagem"

#: app/forms.py:50
msgid "Your message must have between 3 and 4000 characters"
msgstr "A sua mensagem deve ter entre 3 e 4000 caracteres"

#: app/forms.py:51
msgid ""
"Enter your message with suggestions, bug reports or anything else you think "
"is important"
msgstr ""
"Envie sua mensagem com sugestões, erros encontrados ou qualquer outra coisa "
"que você julgar importante"

#: app/views.py:49
msgid "Message successfully sent"
msgstr "Mensagem enviada com sucesso"

#: app/views.py:55
msgid "Your message could not be sent"
msgstr "Sua mensagem não pode ser enviada"

